<div class="cart-header">
    {% if section.settings.cart-logo !=blank %}
        <img src="{{section.settings.cart-logo | img_url: '100x'}}" alt="cart-logo">
    {% else %}
        {{shop.name}}
    {% endif %}
    {% if section.settings.back-img !=blank %}
        <img src="{{section.settings.back-img | img_url: '100x' }}" alt="back">
    {% else %}
        close
    {% endif %}
</div>